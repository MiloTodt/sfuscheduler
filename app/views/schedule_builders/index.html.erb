<script>

    var classes = [];
    var classNum = 0;
    var target = "/schedule_builders/new";
    
    <!-- Add/Remove will only accept text of the form ex. CMPT 320/IAT 100 -->
    
    var reggie = /([A-Z]{4}|[A-Z]{3}|[A-Z]{2})(\s{1})([0-9]{3})([A-Z]?)/;
    

    // if localStorage is supported and if user has his last schedule saved, then show the 'show saved schedule' button
    if (isLocalStorage() && window.localStorage.getItem('savedClasses')) {
        document.getElementById('show-saved-classes-btn').style.opacity = 1;
    }

    function clearBox() {
        for (a=0; a <= 5; a++) {
                classes[a] = "";
                document.getElementById('classBox'+a).innerHTML = "";
            }
    }

    <!-- Updates the list of classes on screen after each addition or removal -->
    
    function showList() {
        for (a=0; a <= classNum; a++) {
                document.getElementById('classBox'+a).innerHTML = classes[a];
            }
    }
    function post(params) { //TODO: This is not working and needs to pass the contents of classes[] to /schedules/builders#create
        var path = "/schedule_builders/new"
    var method =  "post";

 
    var form = document.createElement("form");
    form.setAttribute("method", method);
    form.setAttribute("action", path);

    for(var i = 0 in classNum) {
        form.appendChild(classes[i])
    }

    document.body.appendChild(form);
    form.submit();
}
    
    <!-- Add new class to array classes and update display on page -->
    
    function addClass() {

        var newClass = document.getElementById('tbox').value;
        
        if (reggie.test(newClass) && classes.indexOf(newClass) == -1 && classNum < 6) {
            classes.unshift(newClass);
            showList();
            classNum++;
        }
    }
    
    <!-- Remove class from array classes and update display on page -->
    
    function removeClass(i) {

        var classNum = i;

        classes.splice(classNum, 1);
        showList();
        classNum--;
        
    }

    function createSchedule() {
      var classesOut = classes.splice(0, classNum).join(',');


      document.body.innerHTML += '<form id="dynForm" action ="/schedule_builders/new" method="get"><input type="hidden" name="classes" value="' + classesOut + '"></form>';

      document.getElementById("dynForm").submit();
    }

    function showSavedSchedule() {
      var savedClasses = window.localStorage.getItem('savedClasses').split(',');

      classes = savedClasses;
      classNum = savedClasses.length;

      createSchedule();
    }

    window.onload = clearBox;

    // returns true if localStorage is supported, false otherwise
    function isLocalStorage() {
        if (window.localStorage) {
            var test = "__localstoragetest__";

            try {
                window.localStorage.setItem(test, test);
                window.localStorage.removeItem(test);
            } catch (ex) {
                return false;
            }

            return true;
        }

        return false;
    }
</script>


