<h1>List of Schedules</h1>

<table>
  <thead>
    <tr>
      <th>Classes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules_builders.each do |schedules_builder| %>
      <tr>
        <td><%= schedules_builder.classes %></td>
        <td><%= link_to 'Show', schedules_builder %></td>
